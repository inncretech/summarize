        jQuery('#'+this.id).click(function() { ASINInjectorSaveCursorPos(this); });
        jQuery('#'+this.id).keyup(function() { ASINInjectorSaveCursorPos(this); });
    });

        jQuery("a[name='ASINInjectorTrigger" + elementID + "']").removeAmazonPopoverTrigger();

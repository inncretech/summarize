      title: 'Would you like to add a Customer Care Service Plan for this Product?',


      zIndex: 9900


    });


    


    $('#proceedToCartButton').click(function(e){


      var mainItemId = $("#mainItemId").val();


      var ccpRelatedItemNumber = $("#ccpRelatedItemNumber").val();


      var ccpSelected = $('input:radio[name=RadioGroup1]:checked').val();


      


      $('<input>').attr({


        type : 'hidden',


        name : 'ccp_item',


        value : ccpSelected+ ":" + ccpRelatedItemNumber


      }).appendTo($("#ADD_ITEM_FORM_"+mainItemId));


      


      if(ccpTargetItems.length > 0){


        $('#dialogCCP').dialog('close');


        populateCCPDialog();


      }else{


        submitAddToCartForm();


      }


    });


  


  });


            



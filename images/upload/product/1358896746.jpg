<script type="text/javascript">

  var ccpTargetItems;

  function openCCPDialog(itemId, itemNumber){

    ccpTargetItems = new Array;

    $("#mainItemId").val(itemId);

    $("#ADD_ITEM_FORM_"+itemId+ " input[name=ccp_item]").detach()

    jQuery.getJSON("http://shop.panasonic.com/ecom/service/item.json?item="+ itemNumber, function(data){

      var itemPrice = data.response.prices.shift();      

      if(itemPrice.bundled){
        jQuery.each(itemPrice.components, function(index, componentItemNumber){
          ccpTargetItems.push(componentItemNumber);

        });
                
        $('#proceedToCartButton').attr('class', 'continueBttn');

      }else{

        ccpTargetItems.push(itemNumber);

      }

    }).success(function(){


      populateCCPDialog();


    });      


  }


    


  function populateCCPDialog(){

    $(".radioList").empty();

    var html = '<label>' +

      '<input type="radio" name="RadioGroup1" value="none">' +

      '<p class="descr">I\'d prefer not to add coverage</p>' +

      '<br>' +

    '</label>';

    $(".radioList").append(html);

    var ccpRelatedItemNumber = ccpTargetItems.shift();

    if(ccpTargetItems.length == 0){

      $('#proceedToCartButton').attr('class', 'checkoutBttn');

    }

    $("#ccpRelatedItemNumber").val(ccpRelatedItemNumber);
    var hasCCP = false;

    jQuery.getJSON("/ajax/related-ccp.dot?itemNum="+ ccpRelatedItemNumber, function(data){

      $("#ccpTargetProductImg").attr("src", data.productImageUrl);

      $("#ccpTargetProductModelNumber").empty();

      $("#ccpTargetProductModelNumber").append('Model number: <strong>' + ccpRelatedItemNumber + '</strong>');

      $("#ccpTargetProductShortDescription").empty();        

      $("#ccpTargetProductShortDescription").append(data.ccpTargetProductDescription);

      jQuery.each(data.ccps, function(key, value){

        html = '<label>' +
            '<input type="radio" name="RadioGroup1" value="' + key + '">' +

            '<p class="descr"><a href="#benefits" >' + value.shortDescription +' - ' + value.price + '</a></p>' +

            '<br>' +
          '</label>';

        $(".radioList").append(html);

        hasCCP = true;

      });

    }).complete(function(){


      $('p.descr a').tooltip({ 


        track: true, 


        delay: 2, 


        bodyHandler: function() { 


          return $($(this).attr("href")).html(); 

        },


        showURL: false, 


        fade: 250 


      });


    


      if(!hasCCP){


        submitAddToCartForm();


        return;


      }else{


        $('#dialogCCP').dialog('open');


      }

    });  

  }


  

